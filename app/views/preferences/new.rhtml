<h1>New preference</h1>

<%= error_messages_for :preference %>

<% form_for(:preference, :url => preferences_path) do |f| %>
  <p>
    <b>Mood</b><br />
    <%= f.text_field :mood_id %>
  </p>

  <p>
    <b>Restaurant</b><br />
    <%= f.text_field :restaurant_id %>
  </p>

  <p>
    <b>Value</b><br />
    <%= f.text_field :value %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', preferences_path %>