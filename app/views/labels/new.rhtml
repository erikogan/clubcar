<h1>New label</h1>

<%= error_messages_for :label %>

<% form_for(:label, :url => labels_path) do |f| %>
  <p>
    <b>Restaurant</b><br />
    <%= f.text_field :restaurant_id %>
  </p>

  <p>
    <b>Tag</b><br />
    <%= f.text_field :tag_id %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', labels_path %>