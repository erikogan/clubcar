<h1>Editing label</h1>

<%= error_messages_for :label %>

<% tagged_form_for(:label, :url => label_path(@label), :html => { :method => :put }) do |f| %>
    <%= f.text_field :restaurant_id %>
    <%= f.text_field :tag_id %>
    <%= submit_tag "Update" %>
<% end %>

<%= link_to 'Show', label_path(@label) %> |
<%= link_to 'Back', labels_path %>