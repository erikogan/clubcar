<h1>New mood</h1>

<%= error_messages_for :mood %>

<% form_for(:mood, :url => moods_path) do |f| %> 
  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

 <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Sort order</b><br />
    <%= f.text_field :order %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', moods_path %>