<h1>Listing votes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
  </tr>
  
<% for vote in @votes %>
  <tr>
    <td><%=h vote.name %></td>
    <td><%=h vote.value %></td>
    <td><%= link_to 'Show', vote_path(vote) %></td>
    <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
    <td><%= link_to 'Destroy', vote_path(vote), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New vote', new_vote_path %>