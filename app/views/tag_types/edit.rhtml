<h1>Editing tag_type</h1>

<%= error_messages_for :tag_type %>

<% tagged_form_for(:tag_type, :url => tag_type_path(@tag_type), :html => { :method => :put }) do |f| %>
    <%= f.text_field :name %>
    <%= submit_tag "Update" %>
<% end %>

<%= link_to 'Show', tag_type_path(@tag_type) %> |
<%= link_to 'Back', tag_types_path %>